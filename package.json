{
  "version": "0.1.1-alpha.9",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:risefunds/sdk.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "watch": "npm link && tsdx watch",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "addscope": "node tools/packagejson name @risefunds/sdk"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@risefunds/sdk",
  "author": "Benedict Uyioghosa",
  "module": "dist/sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.5",
    "@types/json-logic-js": "^1.2.1",
    "@types/uuid": "^10.0.0",
    "husky": "^9.1.6",
    "prettier": "3.3.3",
    "size-limit": "^11.1.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.7.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "cross-fetch": "^4.0.0",
    "inversify": "^5.1.1",
    "json-logic-js": "^2.0.5",
    "reflect-metadata": "^0.2.2",
    "urlcat": "^3.1.0",
    "uuid": "^10.0.0"
  }
}
